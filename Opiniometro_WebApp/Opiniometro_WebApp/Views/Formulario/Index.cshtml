@model IEnumerable<Opiniometro_WebApp.Models.Formulario>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<table class="table">
    <tr>
        <th>
            Formularios
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Nombre)
        </td>
        <td>   
            @Html.ActionLink("Details", "Details", new { id=item.CodigoFormulario }) 
        </td>
    </tr>
}

</table>
